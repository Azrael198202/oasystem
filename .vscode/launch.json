            "args": ["-d", "2417B3E5-2E17-4701-9C8F-286A85EE848F"] // chrome、windows、linux、macos、android、ios
        },
        {
            "name": "oa_app",
            "cwd": "app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "oa_app (profile mode)",
            "cwd": "app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "oa_app (release mode)",
            "cwd": "app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}